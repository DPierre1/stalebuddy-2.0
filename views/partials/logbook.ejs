<style>
   #LogBook {
      background-color: transparent;
   }
</style>
<script src="https://use.fontawesome.com/1b82322039.js"></script>
<div><p>LOGBOOK</p></div>
<table id="LogBook" class="table table-inverse">
   <thead id="LogBookHead">
      <tr>
         <th>Product</th>
         <th>Expiration Date</th>
         <th>Comments</th>
         <th>Price</th>
      </tr>
   </thead>
   <tbody id="LogBookBody">
      <% grocery_list.forEach(function(item) { %>
         <tr class="item">
            <td><%= item.product %></td>
            <td><%= item.expiration %></td>
            <td><%= item.comments %></td>
            <td>$<%= item.price %></td>
            <td><a href="#" data-id="@item.Id" class="linkdeleteLogBook"><i class="fa fa-trash-o"></i></a></td>
         </tr>
      <% }); %>
   </tbody>
</table>